<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="frankhaolunliblog.wordpress.com.LapRunner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Lap Runner"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.android.LapRunner"
            android:label="Track Runner"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.android.frankhaolunliblog.wordpress.com.LapRunner.RunActivity"
            android:label="@string/title_activity_run"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>